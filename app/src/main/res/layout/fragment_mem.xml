<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fragment_details_members"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/details_tv_members"
        android:layout_width="81dp"
        android:layout_height="19dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="15dp"
        android:text="3 members"
        android:textAppearance="@style/members"
        />
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/frame_62583"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        app:layout_constraintTop_toBottomOf="@+id/details_tv_members"
        android:clipToOutline="true"
        android:background="@drawable/frame_62583"
        >
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/details_recycle_mem"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:numColumns="1"
            />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>